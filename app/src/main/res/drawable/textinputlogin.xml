<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="2dp" android:bottom="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#F8F8F8" /> <!-- Background color -->
            <corners android:radius="10dp" />  <!-- Radius for rounded corners -->
            <stroke android:width="2dp"  android:color="#E9E9E9" /> <!-- Stroke color -->
        </shape>
    </item>
</layer-list>